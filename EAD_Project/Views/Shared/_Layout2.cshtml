	<script>
		$(document).ready(function () {
			var url = window.location;
			$('.header .container').find('.active').removeClass('active');

			$('.header .container li a').each(function () {
				if (this.href == url) {
					$(this).parent().addClass('active');
				}
			});
		});

	</script>

